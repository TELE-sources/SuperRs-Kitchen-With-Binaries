#!/bin/bash
#
# SuperR's Kitchen v1.x - By SuperR.
#
# Do not edit this file unless you know what you are doing

superr="tools/source/superr"
$superr
answer=""
while [[ $answer != "q" ]]; do
	echo -e "\n\n\033[0m\033[32mPress \"q\" to Quit, or \"r\" to Restart\033[0m"
	read -n 1 -p "" answer
	if [[ $answer = "r" ]]; then
		$superr
	fi
done
exit
